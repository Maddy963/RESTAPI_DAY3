package com.rust.example1.entity;

import javax.persistence.Entity;

public class Student 
{
	private int id;
	private String name;
	private String dept;
	public Student(int id,String name,String dept)
	{
		{
			this.id = id;
			this.name = name;
			this.dept = dept;
		}
		public Student() {
		}
		public int getId() {
			return id;
		}
		public void setId(int id) {
			this.id=id;
		}
		public void setId(String name) {
			this.name=name;
		}
		public void setId(String dept) {
			this.dept=dept;
		}
	}